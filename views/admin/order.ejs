<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Category</title>
  <%- include('./partials/head') -%>
    <link rel="stylesheet" href="/public/admin/css/features/order.css">

<body>
  <!-- Page Preloader -->
  <div id="preloader">
    <div class="spinner-box">
      <div class="blue-orbit leo">
      </div>

      <div class="green-orbit leo">
      </div>

      <div class="red-orbit leo">
      </div>

      <div class="white-orbit w1 leo">
      </div>
      <div class="white-orbit w2 leo">
      </div>
      <div class="white-orbit w3 leo">
      </div>
    </div>
  </div>
  <!-- Toast message -->
  <div id="toast__container"></div>
  <div class="container-fluid">
    <%- include('./partials/nav') -%>
      <div class="container-grid w-95 margin-auto d-grid gap-4">
        <!-- Sidebar Section -->
        <%- include('./partials/sidebar') -%>
          <!-- End of Sidebar Section -->

          <!-- Main Content -->
          <main>
            <div class="flex-between-center mb-2">
              <h2 class="">Order management</h2>
            </div>
            <div>
              <%- include('./order/orderList') -%>
                <%- include('./order/viewOrder') -%>
                  <%- include('./order/shipOrder') -%>
                    <%- include('./order/cancelOrder') -%>
            </div>
          </main>
          <!-- End of Main Content -->

      </div>
  </div>
  <%- include('./partials/footer'); -%>
  <%- include('./partials/script'); -%>
    <script src="/public/admin/js/main.js"></script>
    <script src="/public/admin/js/order.js"></script>
</body>

</html>